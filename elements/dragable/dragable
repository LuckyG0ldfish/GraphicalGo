package dragable

import (
	"github.com/LuckyG0ldfish/GraphicalGo/elements/files"
	"github.com/LuckyG0ldfish/GraphicalGo/elements/objects"
	g "github.com/AllenDang/giu"
)

type Dragable struct{
	

	xPos 	int 
	xRight	int 
	yPos	int 
	yRight	int



	id 		int
	file 	files.File
	obj 	objects.Object
} 

func (D *Dragable) Drag() {
	pos := g.GetCursorPos()
}